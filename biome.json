{
	"$schema": "https://biomejs.dev/schemas/2.1.3/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false
	},
	"formatter": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
    "rules": {
			"recommended": false,
      "complexity": {
        "useArrowFunction": "error"
      }
    }
  }
}
