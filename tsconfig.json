{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "checkJs": false,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "useDefineForClassFields": true,
    "incremental": true,
    "tsBuildInfoFile": "./.tsbuildinfo",
    "types": []
  },
  "include": ["src"],
  "exclude": ["src/components/playback/playbackmanager.js"]
}
