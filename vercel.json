{
  "buildCommand": "npm run build:production",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "nodeVersion": "20.x",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/serviceworker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}

